  "main": "index.js",
  "scripts": {
    "build": "npx -y tsc",
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "test": "mocha -r ts-node/register src/tests/unit/**/*.test.ts --exit -t 60000",
    "test:coverage": "nyc npm run test",
    "db:prestart": "npm run build && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npm run db:prestart"
  },
